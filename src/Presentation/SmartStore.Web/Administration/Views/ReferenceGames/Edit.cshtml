@model SmartStore.Admin.Models.ReferenceGames.ReferenceGame
@{
    //page title
    ViewBag.Title = "Edit Reference Game";
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            Edit Game Reference @Html.ActionLink("(Back to list)", "List")
        </div>
        <div class="options">
            <button type="submit" name="save" value="save" class="btn btn-primary"><i class="fa fa-check"></i>&nbsp;@T("Admin.Common.Save")</button>
            <button type="submit" id="delivery-time-delete" name="delivery-time-delete" class="btn btn-danger"><i class="fa fa-trash-o"></i>&nbsp;@T("Admin.Common.Delete")</button>
        </div>
    </div>
    @Html.Partial("_CreateOrUpdate", Model)
}
